{
	"java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home",
	"java.jdt.ls.vmargs": "-javaagent:/Users/masamonoke/dev/lib/lombok-1.18.34.jar",
	"java.jdt.ls.lombokSupport.enabled": true,
	"java.inlayHints.parameterNames.enabled": "all",
	"clangd.arguments": [
		"--clang-tidy",
		"--header-insertion=never",
		"-j=8",
		"--inlay-hints=true",
		"--background-index",
		"--pch-storage=memory",
	],
	"clangd.checkUpdates": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextCurrentLineOnly": true,
	"[cpp][c]": {
		"semanticTokens.enable": false,
		"semanticTokens.highlightPriority": 100,
	},
	"languageserver": {
		"csharp-ls": {
			"command": "csharp-ls",
			"filetypes": ["cs"],
			"rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
		},
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": ["--lsp"],
			"rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
			"filetypes": ["haskell", "lhaskell"]
		}
	}
}
